<?xml version="1.0" encoding="UTF-8"?>
<!-- Default values for parameters. Do NOT alter this file, use params.local.xml instead -->
<parameters>
	<!-- IP Discovery Application UUID -->
	<discovery_application_uuid>0123-4567-89AB-CDEF</discovery_application_uuid>

	<!-- specify a matching system OID for which serial number detection mechanism to use -->
	<serial_detection type="array">
		<item>
			<!-- HUAWEI-MIB::hwProducts -->
			<system_oid_match>.1.3.6.1.4.1.2011.2</system_oid_match>
			<!-- ENTITY-MIB::entPhysicalSerialNum -->
			<serial_oid>.1.3.6.1.2.1.47.1.1.1.1.11</serial_oid>
			<method>getNextNonEmpty</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- XF-SYSID-MIB::xfNe -->
			<system_oid_match>.1.3.6.1.4.1.193.81.1.1</system_oid_match>
			<!-- ENTITY-MIB::entPhysicalSerialNum.1 -->
			<serial_oid>.1.3.6.1.2.1.47.1.1.1.1.11.1</serial_oid>
			<method>get</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>no</use_as_primary_key>
		</item>
		<item>
			<!-- TIMETRA-GLOBAL-MIB::timetraServiceRouters -->
			<system_oid_match>.1.3.6.1.4.1.6527.1.3</system_oid_match>
			<!-- TIMETRA-CHASSIS-MIB::tmnxHwSerialNumber -->
			<serial_oid>.1.3.6.1.4.1.6527.3.1.2.2.1.8.1.5</serial_oid>
			<method>getNextNonEmpty</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- FORTINET-FORTIGATE-MIB::fgModel -->
			<system_oid_match>.1.3.6.1.4.1.12356.101.1</system_oid_match>
			<!-- FORTINET-CORE-MIB::fnSysSerial.0 -->
			<serial_oid>.1.3.6.1.4.1.12356.100.1.1.0</serial_oid>
			<method>get</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- JUNIPER-SMI::jnxProducts -->
			<system_oid_match>.1.3.6.1.4.1.2636.1</system_oid_match>
			<!-- JUNIPER-MIB::jnxBoxSerialNo.0 -->
			<serial_oid>.1.3.6.1.4.1.2636.3.1.3.0</serial_oid>
			<method>get</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- ACS8000-MIB::acsProduct -->
			<system_oid_match>.1.3.6.1.4.1.10418.26.1</system_oid_match>
			<!-- ACS8000-MIB::acsSerialNumber.0 -->
			<serial_oid>.1.3.6.1.4.1.10418.26.2.1.4.0</serial_oid>
			<method>get</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- ARISTA-SMI-MIB::aristaProducts -->
			<system_oid_match>.1.3.6.1.4.1.30065.1</system_oid_match>
			<!-- ENTITY-MIB::entPhysicalSerialNum -->
			<serial_oid>.1.3.6.1.2.1.47.1.1.1.1.11</serial_oid>
			<method>getNextNonEmpty</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- F5-BIGIP-SYSTEM-MIB::bigipVcmpGuest -->
			<system_oid_match>.1.3.6.1.4.1.3375.2.1.3.4.94</system_oid_match>
			<!-- F5-BIGIP-SYSTEM-MIB::sysGeneralChassisSerialNum.0 -->
			<serial_oid>.1.3.6.1.4.1.3375.2.1.3.3.3.0</serial_oid>
			<method>get</method>
			<use_as_serialnumber>no</use_as_serialnumber>
			<use_as_primary_key>no</use_as_primary_key>
		</item>
		<item>
			<!-- F5-BIGIP-SYSTEM-MIB::sysDeviceModelOIDs -->
			<system_oid_match>.1.3.6.1.4.1.3375.2.1.3.4</system_oid_match>
			<!-- F5-BIGIP-SYSTEM-MIB::sysGeneralChassisSerialNum.0 -->
			<serial_oid>.1.3.6.1.4.1.3375.2.1.3.3.3.0</serial_oid>
			<method>get</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- Oscilloquartz, S.A. -->
			<system_oid_match>.1.3.6.1.4.1.5551</system_oid_match>
			<!-- ENTITY-MIB::entPhysicalSerialNum -->
			<serial_oid>.1.3.6.1.2.1.47.1.1.1.1.11</serial_oid>
			<method>getNextNonEmpty</method>
			<use_as_serialnumber>yes</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
		<item>
			<!-- Match all other systems -->
			<system_oid_match>/.*/</system_oid_match>
			<!-- IF-MIB::ifPhysAddress -->
			<serial_oid>.1.3.6.1.2.1.2.2.1.6</serial_oid>
			<!-- get|getNextNonEmpty|getNextValidMAC -->
			<method>getNextValidMAC</method>
			<use_as_serialnumber>no</use_as_serialnumber>
			<use_as_primary_key>yes</use_as_primary_key>
		</item>
	</serial_detection>

	<sysObjectID_brand_mapping type="array">
		<pattern>/HUAWEI-MIB::/Huawei</pattern>
		<pattern>/XF-\S+-MIB::/Ericsson</pattern>
		<pattern>/TIMETRA-GLOBAL-MIB::/Nokia</pattern>
		<pattern>/FORTINET-FORTIGATE-MIB::/Fortinet</pattern>
		<pattern>/JUNIPER-CHASSIS-DEFINES-MIB::/Juniper</pattern>
		<pattern>/ACS8000-MIB::/Vertiv</pattern>
		<pattern>/ARISTA-PRODUCTS-MIB::/Arista Networks</pattern>
		<pattern>/F5-BIGIP-SYSTEM-MIB::/F5</pattern>
		<!-- Fallback to the enterprise ID if no (specific) MIBs are installed -->
		<pattern>/(?:SNMPv2-SMI::enterprises|\.1\.3\.6\.1\.4\.1)\.(\d+).*/%2$s</pattern>
	</sysObjectID_brand_mapping>

	<sysObjectID_model_mapping type="array">
		<pattern>/HUAWEI-MIB::atn([^.]+)/ATN %2$s</pattern>
		<pattern>/FORTINET-FORTIGATE-MIB::fgt(\w+)/FortiGate %2$s</pattern>
		<pattern>/JUNIPER-CHASSIS-DEFINES-MIB::jnxProductNameJNP(10003|204)/MX %2$s</pattern>
		<pattern>/JUNIPER-CHASSIS-DEFINES-MIB::jnxProduct(?:Name)?(\D+)(\d+)/%2$s %3$s</pattern>
		<pattern>/ACS8000-MIB::acs(\d+)/Avocent ACS %2$s</pattern>
		<pattern>/ARISTA-PRODUCTS-MIB::arista(\w+)$/%2$s</pattern>
		<pattern>/F5-BIGIP-SYSTEM-MIB::bigipVcmpGuest$/BIG-IP vCMP Guest</pattern>
		<pattern>/F5-BIGIP-SYSTEM-MIB::bigipVprB(\w+)$/BIG-IP VIPRION %2$s Blade</pattern>
		<pattern>/F5-BIGIP-SYSTEM-MIB::bigipVprC(\w+)$/BIG-IP VIPRION %2$s Chassis</pattern>
		<pattern>/F5-BIGIP-SYSTEM-MIB::bigip(\w+)$/BIG-IP %2$s</pattern>
		<!-- Use the translated sysObjectId when it doesn't contain indexes -->
		<pattern>/::([^\.]+)$/%2$s</pattern>
	</sysObjectID_model_mapping>

	<sysDescr_brand_mapping type="array">
		<pattern>/Huawei Technologies Co/Huawei</pattern>
		<pattern>/^Avocent/Vertiv</pattern>
		<pattern>/^Arista Networks/%1$s</pattern>
		<pattern>/^BIG-IP/F5</pattern>
		<pattern>/^OSA/ADVA</pattern>
		<pattern>/^Peakflow/Netscout</pattern>
		<pattern>/Copyright \(C\) \d{4}-\d{4} (.+)\.$/%2$s</pattern>
	</sysDescr_brand_mapping>

	<sysDescr_model_mapping type="array">
		<pattern>/HUAWEI (.*)$/%2$s</pattern>
		<pattern>/^MINI-LINK .+/%1$s</pattern>
		<pattern>/Nokia (.+) Copyright/%2$s</pattern>
		<pattern>/^Avocent .+/%1$s</pattern>
		<pattern>/an Arista Networks (\S+)$/%2$s</pattern>
		<pattern>/^BIG-IP VPR-B(\w+) :/BIG-IP VIPRION %2$s Blade</pattern>
		<pattern>/^BIG-IP VPR-C(\w+) :/BIG-IP VIPRION %2$s Chassis</pattern>
		<pattern>/^(BIG-IP [^:]+) :/%2$s</pattern>
		<pattern>/^OSA .+/%1$s</pattern>
	</sysDescr_model_mapping>

	<sysDescr_version_mapping type="array">
		<pattern>/VRP \(R\) software, Version (\S+)/Versatile Routing Platform %2$s</pattern>
		<pattern>/^TiMOS-\S+/%1$s</pattern>
		<pattern>/^Arista Networks EOS version (\S+) running/EOS %2$s</pattern>
		<pattern>/(BIG-IP software release \S+), build/%2$s</pattern>
		<pattern>/^(Peakflow SP \S+) \(build .*\)/%2$s</pattern>
	</sysDescr_version_mapping>

	<!-- default status for newly discovered network devices -->
	<default_status>implementation</default_status>

	<!-- additional network devices to discover -->
	<additional_device_where>status = 'production'</additional_device_where>

	<!-- enable distributed collection -->
	<amqp_enabled>false</amqp_enabled>
	<amqp_host>localhost</amqp_host>
	<amqp_port>5672</amqp_port>
	<amqp_user>guest</amqp_user>
	<amqp_password>guest</amqp_password>

	<!-- enable interface synchronisation -->
	<collect_interfaces>yes</collect_interfaces>

	<json_placeholders>
		<prefix/>
		<uuid>$discovery_application_uuid$</uuid>
		<synchro_status>production</synchro_status>
		<full_load_interval>3600</full_load_interval>
	</json_placeholders>
</parameters>
